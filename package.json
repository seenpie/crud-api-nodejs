{
  "name": "crud-api-nodejs",
  "version": "1.0.0",
  "type": "module",
  "description": "implement nodejs task from https://github.com/AlreadyBored/nodejs-assignments/blob/main/assignments/crud-api/assignment.md",
  "main": "src/index.ts",
  "scripts": {
    "start:dev": "tsx watch --env-file=.env src/index.ts",
    "start:prod": "npm run build && node --env-file=.env dist/bundle.js",
    "build": "webpack --config webpack.config.mjs",
    "lint": "eslint src",
    "lint:fix": "eslint src --fix",
    "format": "prettier src --check",
    "format:fix": "prettier src --write"
  },
  "author": "",
  "license": "ISC",
  "devDependencies": {
    "@eslint/eslintrc": "^3.1.0",
    "@eslint/js": "^9.12.0",
    "@types/uuid": "^10.0.0",
    "@typescript-eslint/eslint-plugin": "^8.9.0",
    "@typescript-eslint/parser": "^8.9.0",
    "eslint": "^9.12.0",
    "eslint-config-prettier": "^9.1.0",
    "eslint-plugin-prettier": "^5.2.1",
    "prettier": "^3.3.3",
    "ts-loader": "^9.5.1",
    "tsx": "^4.19.1",
    "typescript": "^5.6.3",
    "webpack": "^5.95.0",
    "webpack-cli": "^5.1.4"
  },
  "dependencies": {
    "uuid": "^10.0.0"
  }
}
